{
  "mobile": {
    "id": "Spa Demo",
    "title": "Spa Demo",
    "confirmationSettings": {
      "yesText": "Yes",
      "noText": "No"
    },
    "theme": {
      "appearance": "generic",
      "primaryColor": "#3361b7"
    },
    "navigation": {
      "defaultView": "SimpleObject_ListView",
      "items": [
        {
          "icon": "data:image/svg+xml;base64,",
          "title": "SimpleObject",
          "visible": true,
          "id": "SimpleObject_ListView",
          "viewId": "SimpleObject_ListView"
        }
      ]
    },
    "views": [
      {
        "id": "SimpleObject_ListView",
        "type": "listView",
        "title": "Task",
        "components": [
          {
            "id": "viewToolbar",
            "type": "xaf-toolbar",
            "selectionIndependent": [
              {
                "parameterType": "button",
                "text": "Refresh",
                "location": "before",
                "onExecute": {
                  "$action": {
                    "actionId": "Refresh"
                  }
                }
              }
            ]
          },
          {
            "type": "xaf-list",
            "id": "main-list",
            "itemComponents": [
              {
                "type": "stackPanel",
                "id": "verticalPanel",
                "components": [
                  {
                    "type": "label",
                    "text": "$item.Subject"
                  },
                  {
                    "type": "label",
                    "text": "$item.Description"
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "SimpleObject_DetailView$View",
        "type": "detailView",
        "title": "$local.currentObject.Subject",
        "components": [
          {
            "type": "xaf-appbar",
            "selectionIndependent": [
              {
                "type": "command",
                "parameterType": "button",
                "text": "Edit",
                "onExecute": {
                  "$action": {
                    "actionId": "SwitchToEditMode"
                  }
                }
              }
            ]
          },
          {
            "type": "form",
            "id": "mainForm",
            "items": [
              {
                "title": "Subject",
                "type": "label",
                "text": "$local.currentObject.Subject"
              },
              {
                "title": "Description",
                "type": "label",
                "text": "$local.currentObject.Description"
              }
            ]
          }
        ]
      },
      {
        "id": "SimpleObject_DetailView$Edit",
        "type": "detailView",
        "title": "$local.currentObject.Subject",
        "components": [
          {
            "id": "viewToolbar",
            "type": "xaf-appbar",
            "selectionIndependent": [
              {
                "parameterType": "button",
                "text": "Refresh",
                "location": "before",
                "onExecute": {
                  "$action": {
                    "actionId": "Refresh"
                  }
                }
              },
              {
                "type": "command",
                "parameterType": "xaf-singleChoiceAction",
                "items": [
                  {
                    "text": "Save",
                    "onExecute": {
                      "$action": {
                        "actionId": "Save"
                      }
                    }
                  },
                  {
                    "text": "Save&Close",
                    "onExecute": {
                      "$action": {
                        "actionId": "SaveAndClose"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "type": "form",
            "id": "mainForm",
            "items": [
              {
                "title": "Subject",
                "type": "input",
                "value": "$local.currentObject.Subject"
              },
              {
                "title": "Description",
                "type": "input",
                "value": "$local.currentObject.Description"
              }
            ]
          }
        ]
      }
    ]
  },
